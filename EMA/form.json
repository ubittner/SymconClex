				},
				{
					"type": "SelectVariable",
					"name": "Input_Release_SourceVariable",
					"caption": "Quellvariable"
				},
				{
					"type": "Label",
					"caption": "\nBitte wählen Sie die Variable (Schaltaktor) aus, um diesen Eingang des EMA-Moduls zu schalten:"
				},
				{
					"type": "SelectVariable",
					"name": "Input_Release_TargetVariable",
					"caption": "Zielvariable"
				}
